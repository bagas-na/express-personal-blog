{
  "name": "static-personal-blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tailwind:build && tsx ./src/bin/www.ts",
    "devstart": "npm run tailwind:watch & tsx watch ./src/bin/www.ts",
    "tailwind:build": "tailwindcss -i ./public/stylesheets/tailwind.css -o ./public/stylesheets/styles.css --minify",
    "tailwind:watch": "tailwindcss -i ./public/stylesheets/tailwind.css -o ./public/stylesheets/styles.css --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.19.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  }
}
